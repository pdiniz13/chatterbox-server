<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>chatterbox</title>
    <link rel="stylesheet" href="../client/styles/styles.css">

    <!-- dependencies -->
    <script src="../client/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="../client/bower_components/underscore/underscore-min.js"></script>
    <!-- your scripts -->
    <script src="../client/env/config.js"></script>
    <script src="../client/scripts/app.js"></script>
</head>
<body>
<div class="wrapper">
    <div id="main">
        <h1>chatterbox</h1>
        <form action="POST" class="name">
            <input type="text" id="username" placeholder="UserName">
            <input type="password" id="password" placeholder="Password">
            <input type="text" id="FName" placeholder="First Name">
            <input type="text" id="LName" placeholder="Last Name">
            <input type="submit" id="signUp" value="Sign Up">
        </form>
    </div>
</div>
</body>
</html>